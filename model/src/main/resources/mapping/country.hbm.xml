<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="org.acld.model.Country" table="country">
        <id name="id" column="country_id">
            <generator class="native"></generator>
        </id>

        <property name="nombre" column="nombre" type="java.lang.String"></property>
        <property name="code" column="code" type="java.lang.String"></property>
    </class>

</hibernate-mapping>
